<!-- BEGIN TAB_BODY_GENERAL_DATA -->
<table width="80%" border="0" align="center" cellspacing="2" cellpadding="2">
  <tbody>
    <tr>
      <td>
		<table width="80%" border="0" align="center" cellspacing="2" cellpadding="2">
		<tbody>
			<th bgcolor="{th_bg}">{lang_general_data}</th>
			<tr>
			<td valign="top" width="40%">
				<table>
					<tbody>
						<tr bgcolor="{row_on}">
						<td>{lang_prefix}</td>
						<td><input type="text" name="entry[prefix]" value="{prefix}"></td>
						</tr>
						<tr bgcolor="{row_off}">
						<td>{lang_firstname}</td>
						<td><input type="text" name="entry[firstname]" value="{firstname}"></td>
						</tr>
						<tr bgcolor="{row_on}">
						<td>{lang_lastname}</td>
						<td><input type="text" name="entry[lastname]" value="{lastname}"></td>
						</tr>
						<tr bgcolor="{row_off}">
						<td>{lang_middlename}</td>
						<td><input type="text" name="entry[middlename]" value="{middlename}"></td>
						</tr>
						<tr bgcolor="{row_on}">
						<td>{lang_birthday}</td>
						<td>{birthday}</td>
						</tr>
					</tbody>
				</table>
			</td>
			<td valign="top" width="40%">
				<table>
					<tbody>
						<tr bgcolor="{row_on}">
						<td>{lang_address}</td>
						<td><input type="text" name="entry[address]" value="{address}"></td>
						</tr>
						<tr bgcolor="{row_off}">
						<td>{lang_email}</td>
						<td><input type="text" name="entry[email]" value="{email}"></td>
						</tr>
						<tr bgcolor="{row_on}">
						<td>{lang_phone}</td>
						<td><input type="text" name="entry[phone]" value="{phone}"></td>
						</tr>
					</tbody>
				</table>
			</td>
			</tr>
		</tbody>
		</table>
	  </td>
    </tr>

    <tr>
      <td>
		<table width="80%" border="0" align="center" cellspacing="2" cellpadding="2">
		<tbody>
			<tr bgcolor="{row_off}">
			<td width="40%">
				<table>
				<tbody>
					<tr>
					<td>{lang_cats}</td>
					<td>{cats_link}</td>
					</tr>
				</tbody>
				</table>
			</td>
			<td width="40%">
				<table>
				<tbody>
					<tr>
					<td>Orgs</td>
					<td>{orgs_link}</td>
					</tr>
				</tbody>
				</table>
			</td>
			</tr>
		</tbody>
		</table>
	  </td>
    </tr>
  </tbody>
</table>
<!-- END TAB_BODY_GENERAL_DATA -->
