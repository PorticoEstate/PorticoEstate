<!-- $Id$ -->

<xsl:template match="no_access">
	<xsl:choose>
		<xsl:when test="menu != ''">
			<xsl:apply-templates select="menu"/>
		</xsl:when>
	</xsl:choose>
	<table width="100%" cellpadding="2" cellspacing="2" align="center">
		<xsl:choose>
			<xsl:when test="msgbox_data != ''">
				<tr>
					<td align="left" colspan="3">
						<xsl:call-template name="msgbox"/>
					</td>
				</tr>
			</xsl:when>
		</xsl:choose>
	</table>
</xsl:template>
